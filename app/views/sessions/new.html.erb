<%= form_tag ({controller: 'sessions', action: 'create', method: 'post'}) do %>
<%= label_tag(:name, "Your name:") %>
<%= collection_select :user, :name, User.all, :name, :name %> <br>
<%= label_tag(:password, "Your Password:") %>
  <%= password_field_tag :password %>
  <%= submit_tag "Sign In" %>
<% end %>